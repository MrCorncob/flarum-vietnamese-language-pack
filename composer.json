{
  "name": "MrCorncob/flarum-vietnamese-language-pack",
  "version": "1.0.0",
  "description": "Vietnamese language pack (forked from datlechin)",
  "type": "flarum-extension",
  "license": "MIT",
  "keywords": [
    "extention",
    "locale",
    "flarum",
    "Vietnamese",
    "Tieng Viet",
    "translation"
  ],
  "authors": [
    {
      "name": "Ngo Quoc Dat (datlechin)",
      "email": "datlechin@gmail.com",
      "homepage": "https://vieit.net"
    },
    {
      "name": "Yen Le",
      "email": "levu.daiyen@gmail.com",
      "homepage": "https://daiyen.info"
    }
  ],
  "require": {
    "flarum/core": "^0.1.0"
  },
  "extra": {
    "branch-alias": {
        "dev-master": "0.1.x-dev"
    },
    "flarum-extension": {
        "title": "Vietnamese",
        "icon": {
            "image": "icon.svg",
            "backgroundColor": "#00247d",
            "backgroundSize": "cover",
            "backgroundPosition": "center"
        }
    },
    "flarum-locale": {
        "code": "vi",
        "title": "Vietnamese"
    },
    "flagrow": {
      "discuss": "https://discuss.flarum.org/d/22902-vietnamese-extention-for-flarum"
    }
  }
}
